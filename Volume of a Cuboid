//8 kyu
//Instructions
// Bob needs a fast way to calculate the volume of a cuboid with three values: length, width and the height of the cuboid. 
//Write a function to help Bob with this calculation.


//this was basically a math problem!
public class Kata {
  public static double getVolumeOfCubiod(double length, double width, double height) => length * width * height;
}

//test
namespace Solution {
  using NUnit.Framework;
  using System;

  [TestFixture]
  public class SolutionTest
  {
    [Test]
    public void Tests() {
      Assert.AreEqual(60, Kata.getVolumeOfCubiod(2,5,6));
      Assert.AreEqual(94.5, Kata.getVolumeOfCubiod(6.3,3,5));
    }
  }
}
