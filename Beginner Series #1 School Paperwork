//8 kyu
//Instructions: Your classmates asked you to copy some paperwork for them. You know that there are 'n' classmates and the paperwork has 'm' pages.
//Your task is to calculate how many blank pages do you need.

  using System;
  public static class Paper
  {
    public static int Paperwork(int n, int m) => (n > 0 && m > 0) ? (n * m) : 0;
  }

//test
using NUnit.Framework;
  using System;
  
  [TestFixture]
  public class PaperWork
  {
    [Test]
    public void Test()
    {
     Assert.AreEqual(25, Paper.Paperwork(5,5));
    }
  }
