//Instructions: Get the number n (n>0) to return the reversed sequence from n to 1.
//I did this one completely solo!

using System;

public static class Kata
{
  public static int[] ReverseSeq(int n)
  {
    int[] reverse = new int[n];
    int x = 0;
    for (int i = n; i >= 1; i--)
      {
        reverse[x] = i;
        x++;
      }
      return (reverse);
  }
}
